
from array import *

length = int(input("enter the length of your array"))

fst =array('i',[])

for e in range(length):
    x = int(input('enter your number'))
    fst.append(x)


print(fst)

search = int(input("enter the number to search"))
ind=0
for y in fst:
        if(y==search):
             print('position is ',ind)
             break
else:
    print('invalid')

ind+=1
